    "build": "cross-env BUILDING=1 nuxt build",
    "start": "cross-env NODE_ENV=production node -r esm server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.9.3",
