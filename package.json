    "express": "^4.16.4",
    "express-pino-logger": "^4.0.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.5.6",
    "nuxt": "^2.4.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.124",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "vue-jest": "^3.0.3"
  }
