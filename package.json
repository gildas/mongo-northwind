    "express": "^4.17.1",
    "express-pino-logger": "^4.0.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.8.9",
    "nuxt": "^2.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@types/lodash": "^4.14.149",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-jest": "^25.1.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "vue-jest": "^3.0.5"
