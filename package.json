    "express": "^4.17.1",
    "express-pino-logger": "^5.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^5.10.4",
    "nuxt": "^2.14.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@types/lodash": "^4.14.161",
    "@vue/test-utils": "^1.1.0",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "vue-jest": "^3.0.6"
  }
